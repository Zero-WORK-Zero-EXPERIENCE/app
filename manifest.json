{
  "_comment1": "Specifies basic metadata for extension: see https://developer.chrome.com/extensions/manifest for more details",
  "manifest_version": 2,
  "name": "insert project name here",
  "description": "insert project description here ",
  "version": "1.0",
  "author": "insert author names here",

  "browser_action": {
    "default_icon": "",
    "default_popup": ""
  },
  "permissions": [],

  "_comment2": "Icons for 'chrome://extensions'",
  "icons": {
    "16": "/icons/",
    "48": "/icons/",
    "128": "/icons/"
